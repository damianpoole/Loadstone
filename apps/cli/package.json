{
  "name": "loadstone-rs3",
  "version": "0.0.2",
  "description": "CLI for accessing the RuneScape 3 Wiki",
  "bin": {
    "loadstone": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@loadstone/wiki-client": "workspace:*",
    "commander": "^13.1.0",
    "chalk": "^4.1.2",
    "@loadstone/tsconfig": "workspace:*",
    "@loadstone/eslint-config": "workspace:*",
    "@types/bun": "latest",
    "tsup": "^8.5.1",
    "vitest": "^2.1.8",
    "@vitest/ui": "^2.1.8"
  }
}
